{
  "name": "music4",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.9.1",
    "@material-ui/icons": "^4.2.1",
    "@types/jest": "24.0.15",
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.4",
    "axios": "^0.19.0",
    "clsx": "^1.0.4",
    "cross-fetch": "^3.0.4",
    "date-fns": "^1.30.1",
    "electron-is-dev": "^1.1.0",
    "faker": "^4.1.0",
    "formik": "^2.1.4",
    "lodash": "^4.17.15",
    "material-ui-popup-state": "^1.5.3",
    "qs": "^6.9.1",
    "react": "^16.12.0",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-responsive": "^7.0.0",
    "react-scripts": "3.0.1",
    "react-youtube": "^7.9.0",
    "typescript": "3.5.3",
    "use-debounce": "^3.3.0",
    "use-immer": "^0.3.5"
  },
  "scripts": {
    "start": "cross-env BROWSER=none react-scripts start",
    "start-electron": "cross-env ELECTRON_ENV=development electron src/music-electron/index.js",
    "start-electron-prod": "cross-env ELECTRON_ENV=production electron src/music-electron/index.js",
    "start-server": "nodemon",
    "start-server-once": "ts-node --project tsconfig-server.json src/music-server/index.ts",
    "start-server-prod": "cross-env NODE_ENV=production ts-node --project tsconfig-server.json ./src/music-server/index.ts",
    "run-ops": "ts-node --project tsconfig-server.json src/music-ops/index",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@storybook/addon-actions": "^5.1.10",
    "@storybook/addon-links": "^5.1.10",
    "@storybook/addons": "^5.1.10",
    "@storybook/react": "^5.1.10",
    "@types/body-parser": "^1.17.0",
    "@types/faker": "^4.1.5",
    "@types/lodash": "^4.14.146",
    "@types/qs": "^6.9.1",
    "@types/react-responsive": "^3.0.3",
    "@types/react-youtube": "^7.6.2",
    "cross-env": "^5.2.0",
    "electron": "7.0.1",
    "nodemon": "^1.19.1",
    "ts-node": "^8.3.0",
    "utility-types": "^3.7.0"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src/music-server"
    ],
    "exec": "npm run start-server-once",
    "ext": "ts"
  }
}
